#include <stdio.h>
#include <stdlib.h>

typedef struct
{
	int id;
	char nombre[51];
	char apellido[51];
	float salario;
	int sector;
	int isEmpty;

}eEmpleado;

int main(void)
{
	setbuf(stdout,NULL);

	eEmpleado empleado[TAMM];


	return EXIT_SUCCESS;
}
int inicializarEmpleado(eEmpleado empleado[],int tam)
{
	int retorno;
	for(int i=0;i<tam;i++)
	{
		empleado[i].isEmpty=1;
	}

	return retorno;
}
